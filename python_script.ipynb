   "source": [
    "modelling_df = c.execute('''\n",
    "                            SELECT f.DepDelay, f.LateAircraftDelay, f.CarrierDelay, f.NASDelay, f.TaxiOut, f.WeatherDelay, f.ArrDelay\n",
    "                            FROM flight f\n",
    "                            JOIN planes p ON f.TailNum = p.tailnum\n",
    "                            WHERE f.Cancelled = 0 AND f.Diverted = 0\n",
    "                          ''').fetchall()\n",
    "modelling_df = pd.DataFrame(modelling_df, columns=[\"DepDelay\", \"LateAircraftDelay\", \"CarrierDelay\", \"NASDelay\", \"TaxiOut\", \"WeatherDelay\", \"ArrDelay\"])\n",
    "modelling_df"
   ]
