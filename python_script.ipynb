   "source": [
    "flight_freq = c.execute('''\n",
    "                        SELECT flight.Year, flight.Origin, flight.Dest, COUNT(*) as freq\n",
    "                        FROM flight\n",
    "                        GROUP BY flight.Year, flight.Origin, flight.Dest\n",
    "                        ORDER BY freq DESC\n",
    "                        ''').fetchall()\n",
    "\n",
    "flight_freq = pd.DataFrame(flight_freq, columns=['Year', 'Origin', 'Dest', 'Frequency'])\n",
    "flight_freq.head(10)"
   ]
