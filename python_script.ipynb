   "source": [
    "plane_delay = pd.read_sql_query('''\n",
    "            SELECT (f.Year - p.year), \n",
    "                   AVG(ArrDelay)\n",
    "            FROM flight f JOIN planes p ON f.TailNum = p.tailnum\n",
    "            WHERE (f.Year - p.year) >= 0 AND (f.Year - p.year) <= 60 AND ArrDelay > 0\n",
    "            GROUP BY (f.Year - p.year)\n",
    "            ORDER BY (f.Year - p.year)''', conn)\n",
    "\n",
    "plane_delay.columns = [\"Plane Age\", \"DelayTime\"]\n",
    "plane_delay"
   ]
  },
