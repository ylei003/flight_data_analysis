   "source": [
    "# Create a connection to the database.\n",
    "conn = sqlite3.connect('airline.db')\n",
    "\n",
    "# Extract the .csv.bz2 files into .csv files and read them into data frames.\n",
    "flight_2005 = pd.read_csv(\"dataverse_files\\\\2005.csv\")\n",
    "flight_2006 = pd.read_csv(\"dataverse_files\\\\2006.csv\")\n",
    "\n",
    "planes_df = pd.read_csv(\"dataverse_files\\\\plane-data.csv\")\n",
    "carriers_df = pd.read_csv(\"dataverse_files\\\\carriers.csv\")\n",
    "airports_df = pd.read_csv(\"dataverse_files\\\\airports.csv\")\n",
    "\n",
    "# Read data frames to the database.\n",
    "flight_2005.to_sql(\"flight\", con = conn, index = False)\n",
    "conn.commit()\n",
    "flight_2006.to_sql(\"flight\", con = conn, index = False, if_exists = \"append\")\n",
    "conn.commit()\n",
    "\n",
    "planes_df.to_sql(\"planes\", conn, index = False)\n",
    "carriers_df.to_sql(\"carriers\", conn, index = False)\n",
    "airports_df.to_sql(\"airports\", conn, index = False)\n",
    "\n",
    "# Tables in the database include: flight, planes, carriers, airports. (Size of the database: 1064304 KB)"
   ]
