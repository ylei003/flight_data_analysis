   "source": [
    "plane_delay_percent = pd.read_sql_query('''\n",
    "            SELECT (f.Year - p.year), \n",
    "                   ((CAST(SUM(CASE WHEN ArrDelay > 0 THEN 1 ELSE 0 END)AS FLOAT)/CAST(COUNT(*) AS FLOAT))*100)\n",
    "            FROM flight f JOIN planes p ON f.TailNum = p.tailnum\n",
    "            WHERE (f.Year - p.year) >= 0 AND (f.Year - p.year) <= 60\n",
    "            GROUP BY (f.Year - p.year)\n",
    "            ORDER BY (f.Year - p.year)''', conn)\n",
    "\n",
    "plane_delay_percent.columns = [\"Plane Age\", \"DelayPercent\"]\n",
    "plane_delay_percent"
   ]
  },
