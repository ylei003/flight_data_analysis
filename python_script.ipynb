   "source": [
    "### 1c. Best day of week to fly"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "id": "dae1d8ef",
   "metadata": {},
   "outputs": [],
   "source": [
    "delay_percent_day = c.execute('''\n",
    "            SELECT CASE DayOfWeek\n",
    "                       WHEN 1 THEN 'Monday'\n",
    "                       WHEN 2 THEN 'Tuesday'\n",
    "                       WHEN 3 THEN 'Wednesday'\n",
    "                       WHEN 4 THEN 'Thursday'\n",
    "                       WHEN 5 THEN 'Friday'\n",
    "                       WHEN 6 THEN 'Saturday'\n",
    "                       WHEN 7 THEN 'Sunday'\n",
    "                   END AS DayOfWeekName,\n",
    "                   ((CAST(SUM(CASE WHEN ArrDelay > 0 THEN 1 ELSE 0 END)AS FLOAT)/CAST(COUNT(*) AS FLOAT))*100) AS DelayPercent\n",
    "            FROM flight\n",
    "            GROUP BY DayOfWeek\n",
    "            ORDER BY DayOfWeek\n",
    "            ''').fetchall()"
   ]
  },
