    "from sklearn.model_selection import train_test_split\n",
    "from sklearn.pipeline import Pipeline\n",
    "from sklearn.impute import SimpleImputer\n",
    "from sklearn.preprocessing import StandardScaler\n",
    "\n",
    "features = [\"DepDelay\", \"LateAircraftDelay\", \"CarrierDelay\", \"NASDelay\", \"TaxiOut\", \"WeatherDelay\"]\n",
    "numerical_features = [\"DepDelay\", \"LateAircraftDelay\", \"CarrierDelay\", \"NASDelay\", \"TaxiOut\", \"WeatherDelay\"]\n",
    "\n",
    "X = modelling_df[numerical_features].copy()\n",
    "y = modelling_df[\"ArrDelay\"].copy()\n",
    "\n",
    "numerical_transformer = Pipeline(steps=[\n",
    "                                        ('imputer', SimpleImputer()),\n",
    "                                        ('scaler', StandardScaler())])\n",
    "\n",
    "X_train, X_test, y_train, y_test = train_test_split(X, y, test_size = 0.3, random_state = 1)"
   ]
  },
